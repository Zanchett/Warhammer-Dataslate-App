{
  "name": "cyberpunk-terminal",
  "version": "2024.5.24-13.16.41",
  "license": "MIT",
  "homepage": "https://codepen.io/remsrob/pen/ZRyqNx",
  "scripts": {
    "build": "gulp -f ./build/gulpfile.js --cwd ./ --continue",
    "serve": "gulp serve -f ./build/gulpfile.js --cwd ./ --continue",
    "task": "gulp -f ./build/gulpfile.js --cwd ./ --continue",
    "tasks": "gulp --tasks-simple -f ./build/gulpfile.js --cwd ./ --continue"
  },
  "config": {
    "srcDir": "src/",
    "distDir": "dist/"
  },
  "devDependencies": {
    "browser-sync": "^3.0.2",
    "del": "^4.1.1",
    "glob": "^7.1.4",
    "gulp": "^5.0.0",
    "gulp-inject-partials": "^1.0.5",
    "gulp-rename": "^1.4.0",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^5.0.4"
  },
  "browserslist": [
    "last 5 version"
  ],
  "tasks": {
    "partials": {
      "order": 999,
      "src": [
        "**/*.template.html"
      ],
      "pipe": [
        {
          "require": "gulp-inject-partials",
          "args": [
            {
              "removeTags": true,
              "quiet": true,
              "ignoreError": true
            }
          ]
        }
      ],
      "rename": {
        "find": ".template",
        "replace": ""
      },
      "cleanup": [
        "**/*.partial.html",
        "**/*.template.html"
      ]
    }
  },
  "description": "A Pen created on CodePen.io. Original URL: [https://codepen.io/remsrob/pen/ZRyqNx](https://codepen.io/remsrob/pen/ZRyqNx).",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "dependencies": {
    "body-parser": "^1.20.2",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "mongoose": "^8.4.0"
  }
}
